<div id="archive" class="inner">

    <h1>归档</h1>

    <ul>
        <% var year %>
        <% page.posts.forEach(function(post){ %>
        <li>
            <% post.year = date(post.date, 'YYYY') %>
            <% if(post.year !== year) { %>
            <% year = post.year %>
            <h2><%=year%></h2>
            <% } %>
            <a href="<%=config.root + post.path%>"><%=post.title%></a>
        </li>
        <% }); %>
    </ul>

</div>


<%-partial('partials/pagination')%>
